{
  "name": "ickt-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "iview": "2.8.0",
    "less": "2.3.1",
    "less-loader": "^4.1.0",
    "lib-flexible": "^0.3.2",
    "postcss-px-to-viewport": "^0.0.3",
    "postcss-pxtorem": "^4.0.1",
    "vue": "^2.5.13",
    "vue-loader": "14.2.2",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vux": "^2.9.2",
    "vux-loader": "^1.2.9"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-alpha.8",
    "@vue/cli-plugin-eslint": "^3.0.0-alpha.8",
    "@vue/cli-service": "^3.0.0-alpha.8",
    "vue-template-compiler": "^2.5.13"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-px-to-viewport": {
          "viewportWidth": 750,      
          "viewportHeight": 1334,    
          "unitPrecision": 3,        
          "viewportUnit": "vw",     
          "selectorBlackList": [".ignore", ".hairlines"],  
          "minPixelValue": 1,       
          "mediaQuery": false       
      }
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
